{
  "expo": {
    "name": "드랍더넘버 - 로또",
    "slug": "AwesomeLottoProject",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "android": {
      "package": "com.fredooks.lotto",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "NOTIFICATIONS",
        "VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
